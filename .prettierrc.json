{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "semi": true,
    "trailingComma": "none",
    "singleQuote": false,
    "printWidth": 80,
    "importOrder": [
      "^react$",
      "^react",
      "<THIRD_PARTY_MODULES>",
      "interface",
      "(?=content|api)",
      "context/",
      "mock/",
      "config",
      "utils/",
      "hooks/",
      "(components/|./index)",
      ".svg",
      "^../(.*)$",
      "(?=./styles.module.scss)"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true
  }